# 環境変数の例ファイル
# このファイルを .env.development、.env.production、.env.staging としてコピーして使用してください

# API設定
REACT_APP_API_URL=http://localhost:8000
REACT_APP_ENVIRONMENT=development
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_DEV_TOOLS=true

# アプリケーション情報
REACT_APP_VERSION=0.1.0
REACT_APP_BUILD_TIME=development

# 機能設定
REACT_APP_DEBUG_MODE=true
REACT_APP_MOCK_API=false
REACT_APP_ENABLE_LOGGER=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false
REACT_APP_ENABLE_ERROR_TRACKING=true

# セキュリティ設定
REACT_APP_ENABLE_CSP=false
REACT_APP_ENABLE_HSTS=false
REACT_APP_ENABLE_CONTENT_SECURITY_POLICY=false